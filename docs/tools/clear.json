{
  "label": "Clear Web",
  "groups": [
    {
      "label": "Search Engines",
      "engines": [
        { "name": "DuckDuckGo", "url": "https://duckduckgo.com/?q={q}", "desc": "Private search with bangs." },
        { "name": "Startpage", "url": "https://www.startpage.com/do/search?q={q}", "desc": "Google results via proxy." },
        { "name": "Brave Search", "url": "https://search.brave.com/search?q={q}", "desc": "Independent index." },
        { "name": "SearXNG (searx.be)", "url": "https://searx.be/search?q={q}", "desc": "Open-source metasearch." },
        { "name": "Mojeek", "url": "https://www.mojeek.com/search?q={q}", "desc": "Independent crawler." },
        { "name": "Qwant", "url": "https://www.qwant.com/?q={q}", "desc": "EU privacy search." },
        { "name": "Kagi", "url": "https://kagi.com/search?q={q}", "desc": "Subscriber search engine." }
      ]
    },
    {
      "label": "Academic",
      "engines": [
        { "name": "Google Scholar", "url": "https://scholar.google.com/scholar?q={q}", "desc": "Articles & citations." },
