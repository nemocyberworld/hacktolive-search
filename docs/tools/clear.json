        { "name": "Reddit", "url": "https://www.reddit.com/search/?q={q}", "desc": "Community discussions." },
        { "name": "Hacker News (Algolia)", "url": "https://hn.algolia.com/?q={q}", "desc": "HN posts & comments." }
