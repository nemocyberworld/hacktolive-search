        { "name": "PublicWWW (code)", "url": "https://publicwww.com/websites/%22{q}%22/", "desc": "Find strings in sites." },
        { "name": "FilePursuit", "url": "https://filepursuit.com/pursuit?q={q}", "desc": "Public file index." },
        { "name": "filetype:pdf (via DDG)", "url": "https://duckduckgo.com/?q=filetype%3Apdf+{q}", "desc": "Quick PDF filter." }
