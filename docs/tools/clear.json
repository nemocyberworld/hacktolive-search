        { "name": "govinfo (US)", "url": "https://www.govinfo.gov/app/search?q={q}", "desc": "Official publications." },
        { "name": "SEC EDGAR", "url": "https://www.sec.gov/edgar/search/#/q={q}", "desc": "Company filings." },
